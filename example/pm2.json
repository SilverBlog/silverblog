{
  "apps": [
    {
      "name": "SilverBlog",
      "script": "/usr/bin/uwsgi",
      "args": [
        "--json",
        "./uwsgi.json"
      ],
      "merge_logs": true,
      "watch": true,
      "cwd": "./"
    },
    {
      "name": "SilverBlog_Control",
      "script": "/usr/bin/uwsgi",
      "args": [
        "--json",
        "./uwsgi.json:control"
      ],
      "merge_logs": true,
      "cwd": "./"
    }
  ]
}

